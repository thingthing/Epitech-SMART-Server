<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>eip smart server</display-name>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>eip.smart.server.ServerTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>point</servlet-name>
        <servlet-class>eip.smart.server.PointTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>point</servlet-name>
        <url-pattern>/point</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>modeling</servlet-name>
        <servlet-class>eip.smart.server.ModelingTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>modeling</servlet-name>
        <url-pattern>/modeling</url-pattern>
    </servlet-mapping>
</web-app>